@font-face
  font-family: 'vue-player'
  src:  url('../fonts/vue-player.eot?pr0e2e')
  src:  url('../fonts/vue-player.eot?pr0e2e#iefix') format('embedded-opentype'),
        url('../fonts/vue-player.ttf?pr0e2e') format('truetype'),
        url('../fonts/vue-player.woff?pr0e2e') format('woff'),
        url('../fonts/vue-player.svg?pr0e2e#vue-player') format('svg')
  font-weight: normal
  font-style: normal

[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
